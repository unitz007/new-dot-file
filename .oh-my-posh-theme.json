{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "var": {
    "background": "#ffffff",
    "foreground": "#d60dd3"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "#000000",
          "background": "#ffffff",
          "type": "os",
          "style": "accordion"
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "pwsh": "PS"
            }
          },
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#07aa46",
          "type": "shell",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": " \uf0e7 ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b4",
          "background": "#0000ff",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": " {{ .Path }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b4",
          "background": "#0000ff",
          "type": "path",
          "style": "accordion"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }}",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#0fe6c9",
          "type": "git",
          "style": "accordion",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue738 {{ .Full }}",
          "foreground": "#d60dd3",
          "type": "java",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \u202d\ufcd1 {{ .Full }} ",
          "foreground": "#d60dd3",
          "type": "go",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue7a8 {{ .Full }} ",
          "foreground": "#d60dd3",
          "type": "rust",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "context_aliases": {
              "arn:aws:eks:eu-west-1:1234567890:cluster/posh": "posh"
            }
          },
          "template": "{{if .Context}}  \ufd31 {{ .Context }} :: {{ .Namespace }} {{end}}",
          "foreground": "#0ed9b4",
          "type": "kubectl",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "‚ùØ ",
          "foreground": "#D4E157",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#FF5252{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
